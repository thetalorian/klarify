---
structure:
    "generated/state-metrics":
        - "kube-state-metrics"
    "generated/prometheus":
        - "kube-prometheus-stack-"
    "generated/grafana":
        - "kps-grafana"
    "generated/node-exporter":
        - "node-exporter"
categories:
    mesh:
        - Gateway
        - VirtualService
        - DestinationRule
    network:
        - NetworkPolicy
        - Service
        - MutatingWebhookConfiguration
        - ValidatingWebhookConfiguration
    workload:
        - Pod
        - Job
        - Deployment
        - StatefulSet
        - DaemonSet
        - ConfigMap
        - Secret
        - PodSecurityPolicy
        - Prometheus
        - Alertmanager
    rbac:
        - Role
        - RoleBinding
        - ClusterRole
        - ClusterRoleBinding
        - ServiceAccount
    monitoring:
        - PrometheusRule
        - ServiceMonitor
